doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		link(rel="apple-touch-icon" sizes="76x76" href="/nm/paper-dashboard-2/assets/img/apple-icon.png")
		link(rel="icon" type="image/png" href="/nm/paper-dashboard-2/assets/img/favicon.png")
		meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
		title
			| !{title}
		meta(content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport")
		// Fonts and icons
		link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet")
		link(href="/paper-kit/assets/css/all.min.css" rel="stylesheet")
		// CSS Files
		link(href="/nm/paper-dashboard-2/assets/css/bootstrap.min.css" rel="stylesheet")
		link(href="/nm/paper-dashboard-2/assets/css/paper-dashboard.css?v=2.0.0" rel="stylesheet")
		link(href="/css/admin_custom.css" rel="stylesheet")
	body
		.wrapper
			include admin_sidebar
			.main-panel
				include admin_nav
				// content
				block content
				footer.footer.footer-black.footer-white
					.container-fluid
						.row
							.credits.ml-auto
								span.copyright
									| Â© PB UNION made with
									i.fa.fa-heart.heart
									|  by Ho.IT
		// Core JS Files
		script(src="/nm/paper-dashboard-2/assets/js/core/jquery.min.js")
		script(src="/nm/paper-dashboard-2/assets/js/core/popper.min.js")
		script(src="/nm/paper-dashboard-2/assets/js/core/bootstrap.min.js")
		script(src="/nm/paper-dashboard-2/assets/js/plugins/perfect-scrollbar.jquery.min.js")
		// Chart JS
		script(src="/nm/paper-dashboard-2/assets/js/plugins/chartjs.min.js")
		// Notifications Plugin
		script(src="/nm/paper-dashboard-2/assets/js/plugins/bootstrap-notify.js")
		// Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc
		script(src="/nm/paper-dashboard-2/assets/js/paper-dashboard.min.js?v=2.0.0" type="text/javascript")
		block append_js
