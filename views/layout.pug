          span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
          ul.navbar-nav.mr-auto
            li.nav-item.active
              a.nav-link(href="#")
                | Home
                span.sr-only (current)
            li.nav-item
              a.nav-link(href="#") Link
          .form-inline.my-2.my-lg-0
            ul.navbar-nav
              li.nav-item
                a.nav-link(href='#') 로그인
              li.nav-item
                a.nav-link(href='#') 회원가입
      block content


    script(src='/nm/jquery/dist/jquery.min.js', type='text/javascript')
    script(src='/nm/bootstrap/dist/js/bootstrap.bundle.min.js', type='text/javascript')
