doctype html
html
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge, chrome=1')
    meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport')
    title= title
    link(rel='stylesheet', href='/nm/bootstrap/dist/css/bootstrap.css')
    link(rel='stylesheet', type='text/css', href='/nm/slick-slider/slick/slick.css')
